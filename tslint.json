{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": [true, "warning"],
        "quotemark": [true, "single"],
        "trailing-comma": [true, {
            "multiline": "never",
            "singleline": "never"
        }],
        "object-literal-sort-keys": false,
        "arrow-parens": [true, "ban-single-arg-parens"]
    },
    "rulesDirectory": []
}